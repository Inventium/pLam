----------------------------------
--          Fibonacci           --
----------------------------------

:import comp
:import std

-- with Church's numerals
-------------------------------------------------------------------------------
fibC = Y (\f. \x. (or (isZ x) (isZ (Pc x))) 1 (add (f (Pc x)) (f (Pc (Pc x)))))
-------------------------------------------------------------------------------
--| e.g. fibC 4 -> 5

:import binary

-- with binary numerals
---------------------------------------------------------------------------------
fibB = Y (\f. \x. (or (isZ x) (isZ (Pc x))) 1b (addB (f (Pc x)) (f (Pc (Pc x)))))
---------------------------------------------------------------------------------
--| e.g. fibB 5 -> 8bsss
